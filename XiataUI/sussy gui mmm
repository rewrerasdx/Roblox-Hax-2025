		end
		
		buttoncarea.MouseButton1Down:Connect(function()
			ripple(pagefabButton, game.Players.LocalPlayer:GetMouse().X, game.Players.LocalPlayer:GetMouse().Y)
			callbackX2()
		end)
		
		return Button
	end
	
	function Tab:Dropdown(name, array, func)
		local Dropdown = {}
		local reaIarray = array
		local Cback3 = func
		local SeIected = ""
		local pagefabdropdown = Instance.new("Frame")
		local dropdowncorner = Instance.new("UICorner")
		local dropdownimage = Instance.new("ImageLabel")
		local fingergrad = Instance.new("UIGradient")
		local dropdownname = Instance.new("TextLabel")
		local dropdowndivider1 = Instance.new("Frame")
		local dropdowncarea = Instance.new("TextButton")
		local dropdowndivider2 = Instance.new("Frame")
		local dropdowncontent = Instance.new("Frame")
		local dropdowncontent2 = Instance.new("Frame")
		local dropdowncontent2ist = Instance.new("UIListLayout")
		local contentcorner = Instance.new("UICorner")

		pagefabdropdown.Name = "a"
		pagefabdropdown.Parent = page1rea
		pagefabdropdown.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
		pagefabdropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
		pagefabdropdown.BorderSizePixel = 0
		pagefabdropdown.ClipsDescendants = true
		pagefabdropdown.Position = UDim2.new(0.0122282607, 0, 0.196078435, 0)
		pagefabdropdown.Size = UDim2.new(1, -9, 0, 35)

		dropdowncorner.Name = "dropdowncorner"
		dropdowncorner.Parent = pagefabdropdown

		dropdownimage.Name = "dropdownimage"
		dropdownimage.Parent = pagefabdropdown
		dropdownimage.AnchorPoint = Vector2.new(0.5, 0.5)
		dropdownimage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		dropdownimage.BackgroundTransparency = 1.000
		dropdownimage.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdownimage.BorderSizePixel = 0
		dropdownimage.Position = UDim2.new(0, 21, 0, 17)
		dropdownimage.Size = UDim2.new(0, 20, 0, 20)
		dropdownimage.Image = "rbxassetid://104375117000295"

		fingergrad.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(207, 15, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(21, 76, 255))}
		fingergrad.Name = "fingergrad"
		fingergrad.Parent = dropdownimage

		dropdownname.Name = "dropdownname"
		dropdownname.Parent = pagefabdropdown
		dropdownname.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		dropdownname.BackgroundTransparency = 1.000
		dropdownname.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdownname.BorderSizePixel = 0
		dropdownname.Position = UDim2.new(0, 51, 0, -1)
		dropdownname.Size = UDim2.new(0, 148, 0, 35)
		dropdownname.Font = Enum.Font.Arial
		dropdownname.Text = name .. "<font color=\"rgb(100, 100, 100)\"> - None</font>"
		dropdownname.TextColor3 = Color3.fromRGB(255, 255, 255)
		dropdownname.TextSize = 14.000
		dropdownname.TextXAlignment = Enum.TextXAlignment.Left
		dropdownname.RichText = true

		dropdowndivider1.Name = "dropdowndivider1"
		dropdowndivider1.Parent = pagefabdropdown
		dropdowndivider1.BackgroundColor3 = Color3.fromRGB(57, 57, 57)
		dropdowndivider1.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdowndivider1.BorderSizePixel = 0
		dropdowndivider1.Position = UDim2.new(0, 40, 0, 5)
		dropdowndivider1.Size = UDim2.new(0, 1, 0, 24)

		dropdowncarea.Name = "dropdowncarea"
		dropdowncarea.Parent = pagefabdropdown
		dropdowncarea.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		dropdowncarea.BackgroundTransparency = 1.000
		dropdowncarea.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdowncarea.BorderSizePixel = 0
		dropdowncarea.Size = UDim2.new(1, 0, 0, 35)
		dropdowncarea.ZIndex = 2
		dropdowncarea.AutoButtonColor = false
		dropdowncarea.Font = Enum.Font.SourceSans
		dropdowncarea.Text = ""
		dropdowncarea.TextColor3 = Color3.fromRGB(0, 0, 0)
		dropdowncarea.TextSize = 14.000

		dropdowndivider2.Name = "dropdowndivider2"
		dropdowndivider2.Parent = pagefabdropdown
		dropdowndivider2.BackgroundColor3 = Color3.fromRGB(57, 57, 57)
		dropdowndivider2.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdowndivider2.BorderSizePixel = 0
		dropdowndivider2.Position = UDim2.new(0, 6, 0, 34)
		dropdowndivider2.Size = UDim2.new(1, -10, 0, 1)
		dropdowndivider2.Visible = false

		dropdowncontent.Name = "dropdowncontent"
		dropdowncontent.Parent = pagefabdropdown
		dropdowncontent.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
		dropdowncontent.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdowncontent.BorderSizePixel = 0
		dropdowncontent.Position = UDim2.new(0, 11, 0, 41)
		dropdowncontent.Size = UDim2.new(1, -20, 1, -50)
		dropdowncontent.Visible = false
		
		dropdowncontent2.Name = "dropdowncontent"
		dropdowncontent2.AnchorPoint = Vector2.new(0, 0.5)
		dropdowncontent2.Parent = dropdowncontent
		dropdowncontent2.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
		dropdowncontent2.BackgroundTransparency = 1
		dropdowncontent2.BorderColor3 = Color3.fromRGB(0, 0, 0)
		dropdowncontent2.BorderSizePixel = 0
		dropdowncontent2.Size = UDim2.new(1, 0, 1, -15)
		dropdowncontent2.Visible = true
		dropdowncontent2.Position = UDim2.new(0, 0, 0.5, 0)
		
		dropdowncontent2ist.Parent = dropdowncontent2
		dropdowncontent2ist.Padding = UDim.new(0, 5)
		dropdowncontent2ist.HorizontalAlignment = Enum.HorizontalAlignment.Center

		contentcorner.Name = "contentcorner"
		contentcorner.Parent = dropdowncontent

		local dropisopen = false
		local function dropthing()
			dropisopen = not dropisopen
			dropdowndivider2.Visible = dropisopen
			dropdowncontent.Visible = dropisopen
			game:GetService("TweenService"):Create(dropdownimage, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Rotation = dropisopen and 180 or 0}):Play()
			game:GetService("TweenService"):Create(pagefabdropdown, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {Size = dropisopen and UDim2.new(1, -9, 0, 60 + (#reaIarray * 30)) or UDim2.new(1, -9, 0, 35)}):Play()
		end
		dropdowncarea.MouseButton1Click:Connect(dropthing)
		
		function Dropdown:Refresh(newarray)
			reaIarray = newarray
			game:GetService("TweenService"):Create(pagefabdropdown, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {Size = dropisopen and UDim2.new(1, -9, 0, 60 + (#reaIarray * 30)) or UDim2.new(1, -9, 0, 35)}):Play()
			for i, v in dropdowncontent2:GetChildren() do
				if not v:IsA("TextButton") then continue end
				v:Destroy()
			end
			for i, v in newarray do
				local dditem = Instance.new("TextButton")
				local itemcorner = Instance.new("UICorner")

				dditem.Name = "a"
				dditem.Parent = dropdowncontent2
				dditem.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
				dditem.BorderColor3 = Color3.fromRGB(0, 0, 0)
				dditem.BorderSizePixel = 0
				dditem.Position = UDim2.new(0, 7, 0, 0)
				dditem.Size = UDim2.new(1, -75, 0, 25)
				dditem.AutoButtonColor = false
				dditem.Font = Enum.Font.SourceSans
				dditem.Text = tostring(v)
				dditem.TextColor3 = Color3.fromRGB(78, 81, 255)
				dditem.TextSize = 14

				itemcorner.Name = ""
				itemcorner.Parent = dditem
				
				dditem.MouseButton1Click:Connect(function()
					SeIected = v
					dropdownname.Text = name .. "<font color=\"rgb(100, 100, 100)\"> - "..tostring(SeIected).."</font>"
					dropthing()
					Cback3(v)
				end)
			end
		end
		
		function Dropdown:Update(new)
			dropdownname.Text = (new.Name and tostring(new.Name)) or name
			if SeIected ~= "" then
				dropdownname.Text = (new.Name and tostring(new.Name)) .. "<font color=\"rgb(100, 100, 100)\"> - "..tostring(SeIected).."</font>"
			end
			Cback3 = new.Callback or Cback3
		end
		
		function Dropdown:SetVisible(bool)
			pagefabdropdown.Visible = bool
			if dropisopen then
				dropthing()
			end
		end
		
		function Dropdown:GetUsed()
			return SeIected
		end
		
		Dropdown:Refresh(reaIarray)
		
		return Dropdown
	end
	
