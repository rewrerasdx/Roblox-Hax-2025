			M_dropisopen = not M_dropisopen
			M_dropdowndivider2.Visible = M_dropisopen
			M_dropdowncontent.Visible = M_dropisopen
			game:GetService("TweenService"):Create(M_dropdownimage, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Rotation = M_dropisopen and 180 or 0}):Play()
			game:GetService("TweenService"):Create(M_pagefabdropdown, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {Size = M_dropisopen and UDim2.new(1, -9, 0, 60 + (bruhthismany * 30)) or UDim2.new(1, -9, 0, 35)}):Play()
		end
