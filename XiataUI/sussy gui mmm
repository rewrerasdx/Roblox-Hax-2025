		isenabedornot.Position = UDim2.new(0, 21, 0, 17)
		isenabedornot.Size = UDim2.new(0, 20, 0, 20)
		isenabedornot.Image = "rbxassetid://118615604342498"

		toggrad.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(207, 15, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(21, 76, 255))}
		toggrad.Name = "fingergrad"
		toggrad.Parent = isenabedornot
		toggrad.Enabled = false

		toggename.Name = "toggename"
		toggename.Parent = pagefabTogge
		toggename.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		toggename.BackgroundTransparency = 1.000
		toggename.BorderColor3 = Color3.fromRGB(0, 0, 0)
		toggename.BorderSizePixel = 0
		toggename.Position = UDim2.new(0, 51, 0, -1)
		toggename.Size = UDim2.new(0, 148, 0, 35)
		toggename.Font = Enum.Font.Arial
		toggename.Text = name
		toggename.TextColor3 = Color3.fromRGB(255, 255, 255)
		toggename.TextSize = 14.000
		toggename.TextXAlignment = Enum.TextXAlignment.Left

		toggedivider.Name = "toggedivider"
		toggedivider.Parent = pagefabTogge
		toggedivider.BackgroundColor3 = Color3.fromRGB(57, 57, 57)
		toggedivider.BorderColor3 = Color3.fromRGB(0, 0, 0)
		toggedivider.BorderSizePixel = 0
		toggedivider.Position = UDim2.new(0, 40, 0, 5)
		toggedivider.Size = UDim2.new(0, 1, 0, 24)

		toggearea.Name = "toggearea"
		toggearea.Parent = pagefabTogge
		toggearea.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		toggearea.BackgroundTransparency = 1.000
		toggearea.BorderColor3 = Color3.fromRGB(0, 0, 0)
		toggearea.BorderSizePixel = 0
		toggearea.Size = UDim2.new(1, 0, 1, 0)
		toggearea.ZIndex = 2
		toggearea.AutoButtonColor = false
		toggearea.Font = Enum.Font.SourceSans
		toggearea.Text = ""
		toggearea.TextColor3 = Color3.fromRGB(0, 0, 0)
		toggearea.TextSize = 14.000
		
		local toggled = false
		
		local function dotoggle()
			toggled = not toggled
			task.spawn(callbackX, toggled)
			game:GetService("TweenService"):Create(isenabedornot, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(0, 0, 0, 20)}):Play()
			task.wait(0.11)
			toggrad.Enabled = toggled
			isenabedornot.Image = toggled and "rbxassetid://138013952688168" or "rbxassetid://118615604342498"
			game:GetService("TweenService"):Create(isenabedornot, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(0, 20, 0, 20)}):Play()
		end
		
		if default == true then
			dotoggle()
		end
		
		toggearea.MouseButton1Click:Connect(dotoggle)
		
		function Toggle:Enable()
			if not toggled then
				dotoggle()
			end
		end
		
		function Toggle:Disable()
			if toggled then
				dotoggle()
			end
		end
		
		function Toggle:SetVisible(bool)
			pagefabTogge.Visible = bool
		end
		
		function Toggle:GetState()
			return toggled
		end
		
		function Toggle:Update(new)
			toggename.Text = (new.Name and tostring(new.Name)) or name
			callbackX = new.Callback or callbackX
		end
		
		return Toggle
	end
	
