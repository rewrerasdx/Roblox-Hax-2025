		function Dropdown:Refresh(newarray)
			game:GetService("TweenService"):Create(M_pagefabdropdown, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {Size = M_dropisopen and UDim2.new(1, -9, 0, 60 + (bruhthismany * 30)) or UDim2.new(1, -9, 0, 35)}):Play()
			for i, v in M_dropdowncontent2:GetChildren() do
				if not v:IsA("TextButton") then continue end
				v:Destroy()
			end
			bruhthismany = 0
			for i, v in newarray do
				bruhthismany += 1
				print("eeee")
				local M_ididit = false
				local M_dditem = Instance.new("TextButton")
				local M_itemcorner = Instance.new("UICorner")

				M_dditem.Name = "a"
				M_dditem.Parent = M_dropdowncontent2
				M_dditem.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
				M_dditem.BorderColor3 = Color3.fromRGB(0, 0, 0)
				M_dditem.BorderSizePixel = 0
				M_dditem.Position = UDim2.new(0, 7, 0, 0)
				M_dditem.Size = UDim2.new(1, -75, 0, 25)
				M_dditem.AutoButtonColor = false
				M_dditem.Font = Enum.Font.SourceSans
				M_dditem.Text = tostring(i)
				M_dditem.TextColor3 = Color3.fromRGB(78, 81, 255)
				M_dditem.TextSize = 14

				M_itemcorner.Name = ""
				M_itemcorner.Parent = M_dditem

				M_dditem.MouseButton1Click:Connect(function()
					M_ididit = not M_ididit
					store[i] = M_ididit
					M_dditem.Text = (M_ididit and (utf8.char(10003).."\t") or "") .. tostring(i)
					M_dropthing()
					M_Cback3(v)
				end)
			end
		end

		function Dropdown:Update(new)
			M_dropdownname.Text = (new.Name and tostring(new.Name)) or name
			M_Cback3 = new.Callback or M_Cback3
		end

		function Dropdown:SetVisible(bool)
			M_pagefabdropdown.Visible = bool
			if M_dropisopen then
				M_dropthing()
			end
		end

		Dropdown:Refresh(M_reaIarray)

		return Dropdown
	end
	
	function Tab:TextBox(name, ph, func, focuslost, clear, limit)
		local TextBoxX = {}
		local xfunc = func
		local xlimit = limit
		local pagefabtextbox = Instance.new("Frame")
		local textboxcorner = Instance.new("UICorner")
		local textboxname = Instance.new("TextLabel")
		local textboxbox1 = Instance.new("Frame")
		local textinputc = Instance.new("UICorner")
		local TextBox = Instance.new("TextBox")
		local inputstroke = Instance.new("UIStroke")

		pagefabtextbox.Name = "pagefabtextbox"
		pagefabtextbox.Parent = page1rea
		pagefabtextbox.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
		pagefabtextbox.BorderColor3 = Color3.fromRGB(0, 0, 0)
		pagefabtextbox.BorderSizePixel = 0
		pagefabtextbox.ClipsDescendants = true
		pagefabtextbox.Size = UDim2.new(1, -9, 0, 35)

		textboxcorner.Name = "textboxcorner"
		textboxcorner.Parent = pagefabtextbox

		textboxname.Name = "textboxname"
		textboxname.Parent = pagefabtextbox
		textboxname.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		textboxname.BackgroundTransparency = 1.000
		textboxname.BorderColor3 = Color3.fromRGB(0, 0, 0)
		textboxname.BorderSizePixel = 0
		textboxname.Position = UDim2.new(0, 16, 0, 0)
		textboxname.Size = UDim2.new(0, 148, 0, 35)
		textboxname.Font = Enum.Font.Arial
		textboxname.Text = name
		textboxname.TextColor3 = Color3.fromRGB(255, 255, 255)
		textboxname.TextSize = 14.000
		textboxname.TextXAlignment = Enum.TextXAlignment.Left

		textboxbox1.Name = "textboxbox1"
		textboxbox1.Parent = pagefabtextbox
		textboxbox1.AnchorPoint = Vector2.new(1, 0)
		textboxbox1.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
		textboxbox1.BorderColor3 = Color3.fromRGB(0, 0, 0)
		textboxbox1.BorderSizePixel = 0
		textboxbox1.Position = UDim2.new(1, -5, 0.172142684, 0)
		textboxbox1.Size = UDim2.new(0, 59, 0, 22)
		textboxbox1.ClipsDescendants = true

		textinputc.CornerRadius = UDim.new(0, 5)
		textinputc.Name = "textinputc"
		textinputc.Parent = textboxbox1

		TextBox.Parent = textboxbox1
		TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextBox.BackgroundTransparency = 1.000
		TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TextBox.BorderSizePixel = 0
		TextBox.Size = UDim2.new(1, 0, 1, 0)
		TextBox.Font = Enum.Font.Arial
		TextBox.PlaceholderText = ph or "Input"
		TextBox.Text = ""
		TextBox.TextColor3 = Color3.fromRGB(212, 212, 212)
		TextBox.TextSize = 14.000
		TextBox.ClearTextOnFocus = clear
		
		inputstroke.Parent = textboxbox1
		inputstroke.Color = Color3.fromRGB(43, 43, 43)
		
		local Iast = ""
		TextBox.FocusLost:Connect(function()
			Iast = TextBox.Text
			xfunc(TextBox.Text)
			if focuslost == true then TextBox.Text = "" end
		end)
		
		game:GetService("RunService").RenderStepped:Connect(function()
			game:GetService("TweenService"):Create(textboxbox1, TweenInfo.new(0.1, Enum.EasingStyle.Linear), {Size = UDim2.new(0, math.clamp(TextBox.TextBounds.X + 23, 59, pagefabtextbox.AbsoluteSize.X - (textboxname.TextBounds.X + 25)), 0, 22)}):Play()
		end)
		
		TextBox:GetPropertyChangedSignal("Text"):Connect(function()
			if xlimit then
				TextBox.Text = TextBox.Text:sub(1, xlimit)
			end
		end)
		
		function TextBoxX:Update(new)
			textboxname.Text = (new.Name and tostring(new.Name)) or name
			xfunc = new.Func or xfunc
			xlimit = new.limit or xlimit
		end
		
		function TextBoxX:GetPreviousEntery()
			return Iast
		end
		
		return TextBoxX
	end
	
	return Tab
end

function ary:Update(new)
	TextLabel.Text = "  "..((new.Name and tostring(new.Name)) or TextLabel.Text)
	TextLabel_2.Text = ((new.ExtraText and tostring(new.ExtraText)) or TextLabel_2.Text)
	return ary
end

return ary
