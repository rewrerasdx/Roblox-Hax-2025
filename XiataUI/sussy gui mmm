		isenabedornot.Parent = pagefabTogge
		isenabedornot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		isenabedornot.BackgroundTransparency = 1.000
		isenabedornot.BorderColor3 = Color3.fromRGB(0, 0, 0)
