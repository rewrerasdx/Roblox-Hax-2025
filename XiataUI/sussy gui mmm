--Properties:

ScreenGui.Parent = game:GetService("RunService"):IsStudio() and game.Players.LocalPlayer:WaitForChild("PlayerGui") or (gethui and gethui() or game:GetService("CoreGui"))
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
ScreenGui.DisplayOrder = 9999999
if not game:GetService("RunService"):IsStudio() then
	ScreenGui.OnTopOfCoreBlur = true
end
function ripple(obj, x, y)
	spawn(function()
		obj.ClipsDescendants = true
		local circle = Instance.new("ImageLabel")
		circle.Name = "Circle"
		circle.Parent = obj
		circle.BackgroundColor3 = Color3.new(1, 1, 1)
		circle.BackgroundTransparency = 1
		circle.Image = "rbxassetid://266543268"
		circle.ImageTransparency = 0.5

		circle.Parent = obj
		circle.ZIndex = 1000

		local new_x = x - circle.AbsolutePosition.X
		local new_y = y - circle.AbsolutePosition.Y
		circle.Position = UDim2.new(0, new_x, 0, new_y)

		local size = 0
		if obj.AbsoluteSize.X > obj.AbsoluteSize.Y then
			size = obj.AbsoluteSize.X * 1.5
		elseif obj.AbsoluteSize.X < obj.AbsoluteSize.Y then
			size = obj.AbsoluteSize.Y * 1.5
		elseif obj.AbsoluteSize.X == obj.AbsoluteSize.Y then
			size = obj.AbsoluteSize.X * 1.5
		end

		circle:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, -size / 2, 0.5, -size / 2), "Out", "Quad", 0.5, false, nil)
		game:GetService("TweenService"):Create(circle, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()

		wait(0.5)
		circle:Destroy()
	end)
end

