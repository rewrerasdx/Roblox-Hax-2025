function ary:Notify(text, duration)
	local notification1 = Instance.new("Frame")
	local UICorner_9 = Instance.new("UICorner")
	local DropShadowHolder_2 = Instance.new("Frame")
	local DropShadow_2 = Instance.new("ImageLabel")
	local ImageLabel_2 = Instance.new("ImageLabel")
	local TextLabel_3 = Instance.new("TextLabel")
	
	notification1.Name = "notification1"
	notification1.Parent = notifications
	notification1.AnchorPoint = Vector2.new(0.5, 0.5)
	notification1.BackgroundColor3 = Color3.fromRGB(17, 17, 17)
	notification1.BorderColor3 = Color3.fromRGB(0, 0, 0)
	notification1.BorderSizePixel = 0
	notification1.Position = UDim2.new(0, 0, 0.946648419, 0)
	notification1.Size = UDim2.new(0, 0, 0, 0)
	notification1.ClipsDescendants = true

	UICorner_9.CornerRadius = UDim.new(0, 10)
	UICorner_9.Parent = notification1

	DropShadowHolder_2.Name = "DropShadowHolder"
	--DropShadowHolder_2.Parent = notification1
	DropShadowHolder_2.BackgroundTransparency = 1.000
	DropShadowHolder_2.BorderSizePixel = 0
	DropShadowHolder_2.Size = UDim2.new(1, 0, 1, 0)
	DropShadowHolder_2.ZIndex = 0

	DropShadow_2.Name = "DropShadow"
	DropShadow_2.Parent = DropShadowHolder_2
	DropShadow_2.AnchorPoint = Vector2.new(0.5, 0.5)
	DropShadow_2.BackgroundTransparency = 1.000
	DropShadow_2.BorderSizePixel = 0
	DropShadow_2.Position = UDim2.new(0.5, 0, 0.5, 0)
	DropShadow_2.Size = UDim2.new(1, 41, 1, 41)
	DropShadow_2.ZIndex = 0
	DropShadow_2.Image = "rbxassetid://6014261993"
	DropShadow_2.ImageColor3 = Color3.fromRGB(0, 0, 0)
	DropShadow_2.ImageTransparency = 0.500
	DropShadow_2.ScaleType = Enum.ScaleType.Slice
	DropShadow_2.SliceCenter = Rect.new(49, 49, 450, 450)

	ImageLabel_2.Parent = notification1
	ImageLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageLabel_2.BackgroundTransparency = 1.000
	ImageLabel_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ImageLabel_2.BorderSizePixel = 0
	ImageLabel_2.Position = UDim2.new(0, 5, 0, 9)
	ImageLabel_2.Size = UDim2.new(0, 20, 0, 20)
	ImageLabel_2.Image = "rbxassetid://98886205034264"
	ImageLabel_2.ImageColor3 = Color3.fromRGB(255, 57, 60)

	TextLabel_3.Parent = notification1
	TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_3.BackgroundTransparency = 1.000
	TextLabel_3.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel_3.BorderSizePixel = 0
	TextLabel_3.Position = UDim2.new(0, 34, 0, 0)
	TextLabel_3.Size = UDim2.new(1, -33, 1, 0)
	TextLabel_3.Font = Enum.Font.Ubuntu
	TextLabel_3.Text = text
	TextLabel_3.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_3.TextSize = 16.000
	TextLabel_3.TextXAlignment = Enum.TextXAlignment.Left
	
	
	game:GetService("TweenService"):Create(notification1, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {Size = UDim2.new(0, TextLabel_3.TextBounds.X + 42, 0, 39)}):Play()
	
	task.spawn(function()
		wait(duration or 10)
		game:GetService("TweenService"):Create(notification1, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {Size = UDim2.new(0, 0, 0, 0)}):Play()
		task.wait(0.21)
		notification1:Destroy()
	end)
end


function gm()
	return game.Players.LocalPlayer:GetMouse()
end

local MinX, MinY = 700, 300
function csize(bmenu)
	game:GetService("TweenService"):Create(main, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(main.Size.X.Scale, math.clamp(gm().X - main.AbsolutePosition.X, MinX, 9999), main.Size.Y.Scale, math.clamp(gm().Y - main.AbsolutePosition.Y, MinY, 9999))}):Play()
	if bmenu then	
		if main.Size.X.Offset < 400 then
			menus.Visible = false
		else
			menus.Visible = true
		end
	end
end

function ary:SetMaxSize(x, y)
	MinX, MinY = (x or MinX), (y or MinY)
	if main.Size.X.Offset < x then
		main.Size = UDim2.new(main.Size.X.Scale, x, main.Size.Y.Scale, main.Size.Y.Offset)
	elseif main.Size.Y.Offset < y then
		main.Size = UDim2.new(main.Size.X.Scale, main.Size.X.Offset, main.Size.Y.Scale, y)
	end
end

function ary:ChangeSize(x, y)
	print(x, y)
	if x then
		game:GetService("TweenService"):Create(main, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(main.Size.X.Scale, x, main.Size.Y.Scale, main.Size.Y.Offset)}):Play()
	end
	if y then
		game:GetService("TweenService"):Create(main, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(main.Size.X.Scale, main.Size.X.Offset, main.Size.Y.Scale, y)}):Play()
	end
end

function msize()
	main:TweenSizeAndPosition(
		UDim2.new(main.Size.X.Scale, ScreenGui.AbsoluteSize.X, main.Size.Y.Scale, ScreenGui.AbsoluteSize.Y),
		UDim2.new(0.5, 0, 0.5, 0),
		Enum.EasingDirection.InOut,
		Enum.EasingStyle.Quart,
		0.26,
		true
	)
end

isheld = false

--[[Resizer.MouseEnter:Connect(function()
	Resizer.ImageTransparency = 0
end)

Resizer.MouseLeave:Connect(function()
	if not isheld then
		Resizer.ImageTransparency = 1
	end
end)]]

Resizer.MouseButton1Down:Connect(function()
	isheld = true
	while isheld do
		csize(false)
		task.wait()
	end
end)

Resizer.MouseButton1Up:Connect(function()
	isheld = false
	Resizer.ImageTransparency = 1
end)

game:GetService("UserInputService").InputEnded:Connect(function(k)
	if k.UserInputType == Enum.UserInputType.MouseButton1 then
		isheld = false
		Resizer.ImageTransparency = 1
	end
end)

MaxCickButton.MouseButton1Click:Connect(msize)

-- // dragify

local UserInputService = game:GetService("UserInputService")

local gui = main

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	game:GetService("TweenService"):Create(gui, TweenInfo.new(0.05, Enum.EasingStyle.Sine), {Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)}):Play()
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

function ary:Tab(tite)
	local Tab = {}
	
	local page1 = Instance.new("Frame")
	local page1rea = Instance.new("ScrollingFrame")
	
	function Tab:Seperator(name, sp)
		local frame_x = Instance.new("Frame")
		local grad_x = Instance.new("UIGradient")
		local tabe_x = Instance.new("TextLabel")
