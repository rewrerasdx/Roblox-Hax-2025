		frame_x.AnchorPoint = Vector2.new(0.5, 0)
		frame_x.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		frame_x.BorderColor3 = Color3.fromRGB(0, 0, 0)
		frame_x.BorderSizePixel = 0
		frame_x.Position = UDim2.new(0.5, 0, 0, 15)
