	TextButton.TextColor3 = Color3.fromRGB(100, 100, 100)
	TextButton.TextSize = 15

	UIGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 6, 251)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(2, 69, 255))}
	UIGradient_2.Parent = TextButton
	UIGradient_2.Enabled = false
	
	imisyes.Name = "imisyes"
	imisyes.Parent = atabunseected
	imisyes.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	imisyes.BorderColor3 = Color3.fromRGB(0, 0, 0)
	imisyes.BorderSizePixel = 0
	imisyes.Position = UDim2.new(0, 0, 0.84375, 0)
	imisyes.Size = UDim2.new(0, 0, 0, 1)
	
	amountofmenus = amountofmenus + 1
	table.insert(tabelm, {TextButton, page1})
	local thetabisenabled = false
	local function tabuhh()
		for i, v in tabelm do
			if v ~= TextButton then
				v[1].TextColor3 = Color3.fromRGB(100, 100, 100)
				v[1].UIGradient.Enabled = false
				v[2].Visible = false
				game:GetService("TweenService"):Create(v[1], TweenInfo.new(0.1, Enum.EasingStyle.Sine), {TextSize = 15}):Play()
				game:GetService("TweenService"):Create(v[1].Parent.imisyes, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(0, 0, 0, 1)}):Play()
			end
		end
		TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		game:GetService("TweenService"):Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {TextSize = 16}):Play()
		game:GetService("TweenService"):Create(imisyes, TweenInfo.new(0.1, Enum.EasingStyle.Sine), {Size = UDim2.new(1, 0, 0, 1)}):Play()
		UIGradient_2.Enabled = true
		page1.Visible = true
	end
	if amountofmenus == 1 then tabuhh() end
	
	TextButton.MouseButton1Click:Connect(tabuhh)
	
	function Tab:Text(text, customprops)
		local cprops = customprops or {}
		local label = {}
		
		local TextLabel = Instance.new("TextLabel")

		TextLabel.Parent = page1rea
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.BorderSizePixel = 0
		TextLabel.Position = UDim2.new(0.0279255323, 0, 0.427010089, 0)
		TextLabel.Size = UDim2.new(1, -9, 0, 15)
		TextLabel.Font = Enum.Font.SourceSans
		TextLabel.Text = text
		TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.TextSize = 14.000
		TextLabel.TextXAlignment = Enum.TextXAlignment.Left
		
		for i, v in customprops do
			if tostring(i) == "Parent" then return end
			TextLabel[tostring(i)] = v
		end
		
		function label:Update(new)
			TextLabel.Text = (new.Text and tostring(new.Text)) or text
		end
		
		return label
	end
	
	function Tab:Slider(name, min, max, callback)
		local slider = {}
		local callbackX = callback or function() end
		local pagefabsider = Instance.new("Frame")
		local sidercorner = Instance.new("UICorner")
		local sidername = Instance.new("TextLabel")
		local siderdivider = Instance.new("Frame")
		local sidervaue = Instance.new("TextBox")
		local sidercontainer = Instance.new("Frame")
		local contcorner = Instance.new("UICorner")
		local incontframe = Instance.new("Frame")
		local incontcorner = Instance.new("UICorner")
		local siderinputter = Instance.new("TextButton")

		pagefabsider.Name = "a"
		pagefabsider.Parent = page1rea
		pagefabsider.BackgroundColor3 = Color3.fromRGB(32, 32, 32)
		pagefabsider.BorderColor3 = Color3.fromRGB(0, 0, 0)
		pagefabsider.BorderSizePixel = 0
		pagefabsider.ClipsDescendants = true
		pagefabsider.Size = UDim2.new(1, -9, 0, 35)

		sidercorner.Name = "sidercorner"
		sidercorner.Parent = pagefabsider

		sidername.Name = name
		sidername.Parent = pagefabsider
		sidername.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		sidername.BackgroundTransparency = 1.000
		sidername.BorderColor3 = Color3.fromRGB(0, 0, 0)
		sidername.BorderSizePixel = 0
		sidername.Position = UDim2.new(0, 51, 0, 0)
		sidername.Size = UDim2.new(0, 148, 0, 35)
		sidername.Font = Enum.Font.Arial
		sidername.Text = name
		sidername.TextColor3 = Color3.fromRGB(255, 255, 255)
		sidername.TextSize = 14.000
		sidername.TextXAlignment = Enum.TextXAlignment.Left

		siderdivider.Name = "siderdivider"
		siderdivider.Parent = pagefabsider
		siderdivider.BackgroundColor3 = Color3.fromRGB(57, 57, 57)
		siderdivider.BorderColor3 = Color3.fromRGB(0, 0, 0)
		siderdivider.BorderSizePixel = 0
		siderdivider.Position = UDim2.new(0, 40, 0, 5)
		siderdivider.Size = UDim2.new(0, 1, 0, 24)

		sidervaue.Name = "sidervaue"
		sidervaue.Parent = pagefabsider
		sidervaue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		sidervaue.BackgroundTransparency = 1.000
		sidervaue.BorderColor3 = Color3.fromRGB(0, 0, 0)
		sidervaue.BorderSizePixel = 0
		sidervaue.Position = UDim2.new(0, 9, 0, 0)
		sidervaue.Size = UDim2.new(0, 24, 0, 35)
		sidervaue.Font = Enum.Font.Arial
		sidervaue.Text = min
		sidervaue.TextColor3 = Color3.fromRGB(103, 103, 103)
		sidervaue.TextSize = 14.000

		sidercontainer.Name = "sidercontainer"
		sidercontainer.Parent = pagefabsider
		sidercontainer.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
		sidercontainer.BorderColor3 = Color3.fromRGB(0, 0, 0)
		sidercontainer.BorderSizePixel = 0
		sidercontainer.Position = UDim2.new(1, -175, 0.286, 0)
		sidercontainer.Size = UDim2.new(0, 163, 0, 15)

		contcorner.CornerRadius = UDim.new(0, 7)
		contcorner.Name = "contcorner"
		contcorner.Parent = sidercontainer

		incontframe.Name = "incontframe"
		incontframe.Parent = sidercontainer
		incontframe.BackgroundColor3 = Color3.fromRGB(41, 41, 41)
		incontframe.BorderColor3 = Color3.fromRGB(0, 0, 0)
		incontframe.BorderSizePixel = 0
		incontframe.Size = UDim2.new(0, 0, 1, 0)

		incontcorner.CornerRadius = UDim.new(0, 7)
		incontcorner.Name = "incontcorner"
		incontcorner.Parent = incontframe

		siderinputter.Name = "siderinputter"
		siderinputter.Parent = sidercontainer
		siderinputter.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		siderinputter.BackgroundTransparency = 1.000
		siderinputter.BorderColor3 = Color3.fromRGB(0, 0, 0)
		siderinputter.BorderSizePixel = 0
		siderinputter.Size = UDim2.new(1, 0, 1, 0)
		siderinputter.Font = Enum.Font.SourceSans
		siderinputter.Text = ""
		siderinputter.TextColor3 = Color3.fromRGB(0, 0, 0)
		siderinputter.TextSize = 14.000
		
		local dragging = false
		local svaue = min
		
		function slider:Set(newv)
			local meas = math.clamp((newv - min) / (max - min), 0, 1)
			game:GetService("TweenService"):Create(incontframe, TweenInfo.new(0.15, Enum.EasingStyle.Quint), {Size = UDim2.new(meas, 0, 1, 0)}):Play()
			sidervaue.Text = newv
			svaue = newv
			callbackX(newv)
		end
		
		siderinputter.MouseButton1Down:Connect(function()
			dragging = true
			while dragging do
				meas = math.clamp((game:GetService("Players").LocalPlayer:GetMouse().X-sidercontainer.AbsolutePosition.X)/sidercontainer.AbsoluteSize.X, 0, 1)
				newsv = min + math.round((meas * (max - min)))
				slider:Set(newsv)
				task.wait()
			end
		end)
		
		--[[siderinputter.MouseButton1Up:Connect(function()
			dragging = false
		end)]]
		
		game:GetService("UserInputService").InputEnded:Connect(function(e)
			if e.UserInputState == Enum.UserInputState.End and e.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false
			end
		end)
		
		function slider:Update(new)
			sidername.Text = (new.Name and tostring(new.Name)) or name
			callbackX = new.Callback or callbackX
		end
		
		sidervaue.FocusLost:Connect(function()
			if tonumber(sidervaue.Text) then
				slider:Set(tonumber(sidervaue.Text))
			end
		end)
		
		return slider
	end
	
	function Tab:Toggle(name, callback, default)
		local Toggle = {}
		local callbackX = callback
		local pagefabTogge = Instance.new("Frame")
		local togcorner = Instance.new("UICorner")
		local isenabedornot = Instance.new("ImageLabel")
